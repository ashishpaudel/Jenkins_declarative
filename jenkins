pipeline{
    agent any
    stages{
        stage("Build"){
            step{
                echo "This is Building process."
            }
        }

        stage("Test"){
            step{
                echo "This is Testing process."
            }
        }

        stage("Deploy"){
            step{
                echo "This is Deploying process."
            }
        }
        stage("Relase"){
            step{
                echo "This is Relasing process."
            }
        }
    }
}